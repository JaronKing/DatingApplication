<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    <title>Starter Template for Bootstrap</title>

    <!-- Bootstrap core CSS -->
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" integrity="sha512-dTfge/zgoMYpP7QbHy4gWMEGsbsdZeCXz7irItjcC3sPUFtf0kuFbDz/ixG7ArTxmDjLXDmezHubeNikyKGVyQ==" crossorigin="anonymous">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css" integrity="sha384-aUGj/X2zp5rLCbBxumKTCw2Z50WgIr1vs/PFN4praOTvYXWlVyh2UtNUU0KAUhAX" crossorigin="anonymous">

    {%  stylesheets
    'bundles/datingmain/dashgumfree/assets/css/bootstrap.css'
    'bundles/datingmain/dashgumfree/assets/font-awesome/css/font-awesome.css'
    'bundles/datingmain/dashgumfree/assets/css/zabuto_calendar.css'
    'bundles/datingmain/dashgumfree/assets/js/gritter/css/jquery.gritter.css'
    'bundles/datingmain/dashgumfree/assets/lineicons/style.css'
    'bundles/datingmain/dashgumfree/assets/css/style.css'
    'bundles/datingmain/dashgumfree/assets/css/style-responsive.css'
    filter='cssrewrite'
    output='assets/css/compiled/main.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
    {% block stylesheets %}
    {% endblock stylesheets %}
</head>

<body>
<style scoped>
    body {
        padding-top: 50px;
    }
    .starter-template {
        padding: 40px 15px;
        text-align: center;
    }
</style>
<nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('dating_main_homepage') }}">Dating Application</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li class="active"><a href="{{ path('dating_main_homepage') }}">Home</a></li>
            {% if app.user %}
                <li><a href="{{ path('user_profile') }}">Profile</a></li>
                <li><a href="">Match</a></li>
                <li><a href="">Survey</a></li>
                <li><a href="{{ path('user_logout') }}">Logout</a></li>
            {% else %}
                <li><a href="{{ path('admin_login') }}">Admin Login</a></li>
                <li><a href="{{ path('user_login') }}">Login</a></li>
                <li><a href="{{ path('user_register') }}">Register</a></li>
            {% endif %}
            {% if is_granted('ROLE_ADMIN') %}
                <li><a href="{{ path('dating_admin_homepage') }}">Admin</a></li>
            {% endif %}
            </ul>
        </div><!--/.nav-collapse -->
    </div>
</nav>


<div class="container">
    <div class="starter-template">
        {% block body %}{% endblock %}
    </div>
</div>

<!-- Bootstrap core JavaScript
================================================== -->
<!-- Latest compiled and minified JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" integrity="sha512-K1qjQ+NcF2TYO/eI3M6v8EiNYZfA95pQumfvcVrTHtwQVDG+aHRqLi/ETn2uB+1JqwYqVG3LIvdm9lj6imS/pQ==" crossorigin="anonymous"></script>
{% javascripts
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/chart-master/Chart.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/jquery.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/jquery-1.8.3.min.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/bootstrap.min.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/jquery.dcjqaccordion.2.7.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/jquery.scrollTo.min.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/jquery.nicescroll.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/jquery.sparkline.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/common-scripts.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/gritter/js/jquery.gritter.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/gritter-conf.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/sparkline-chart.js'
'@DatingMainBundle/Resources/public/dashgumfree/assets/js/zabuto_calendar.js'
output='assets/js/compiled/main.js'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
{% block javascripts %}

{% endblock javascripts %}
</body>
</html>
